{
    "id": "ordem-paranormal",
    "title": "Ordem Paranormal",
    "description": "A community contributed game system for Pathfinder Second Edition",
    "license": "./LICENSE",
    "version": "0.9",
    "compatibility": {
        "minimum": "10.286",
        "verified": "10.286",
        "maximum": "10"
    },
    "authors": [
        {
            "name": "The PF2e System Developers",
            "url": "https://github.com/foundryvtt/pf2e"
        }
    ],
    "esmodules": ["vendor.bundle.js", "main.bundle.js"],
    "scripts": ["./lib/tooltipster.bundle.min.js"],
    "styles": ["./styles/main.css"],
    "packs": [
        {
            "name": "pf2e-macros",
            "label": "Macros",
            "system": "ordem-paranormal",
            "path": "packs/macros.db",
            "type": "Macro"
        }
    ],
    "languages": [
        {
            "lang": "en",
            "name": "Main (English)",
            "path": "lang/en.json"
        },
        {
            "lang": "en",
            "name": "Rules Elements (English)",
            "path": "lang/re-en.json"
        }
    ],
    "socket": true,
    "templateVersion": 2,
    "initiative": "(@abilities.wis.value)d20 + @attributes.perception.value + (@abilities.wis.value / 100)",
    "gridDistance": 1.5,
    "gridUnits": "m",
    "primaryTokenAttribute": "attributes.hp",
    "secondaryTokenAttribute": "attributes.pe",
    "url": "https://github.com/foundryvtt/pf2e",
    "bugs": "https://github.com/foundryvtt/pf2e/issues",
    "changelog": "https://github.com/foundryvtt/pf2e/blob/release/CHANGELOG.md",
    "manifest": "https://github.com/foundryvtt/pf2e/releases/latest/download/system.json",
    "download": "https://github.com/foundryvtt/pf2e/releases/latest/download/pf2e.zip"
}
